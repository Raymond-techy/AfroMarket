{
  "expo": {
    "name": "Afromarket",
    "slug": "Afromarket",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/AfromarketLogo2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/Splash.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
